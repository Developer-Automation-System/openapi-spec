            "required": true,
            "type": "string"
          },
          {
            "name": "academicPeriod",
            "in": "query",
            "description": "The academic period associated with this object. When the object is associated with both a starting and an ending academic period, such as a financial aid period record, this report field returns the starting academic period.",
            "required": false,
            "type": "string"
          },
          {
            "name": "fromDate",
            "in": "query",
            "description": "Limit student payments to payments made on or after this date, using the yyyy-mm-dd format.",
            "required": false,
            "type": "string",
            "format": "date"
          },
          {
            "name": "institutionalAcademicUnit",
            "in": "query",
            "description": "The academic unit, an organizational type for academic appointments.",
            "required": false,
            "type": "string"
          },
          {
            "name": "limit",
            "in": "query",
            "description": "The maximum number of objects in a single response. The default is 20. The maximum is 100.",
            "required": false,
            "type": "integer",
            "format": "int64"
          },
          {
            "name": "offset",
            "in": "query",
            "description": "The zero-based index of the first object in a response collection. The default is 0. Use offset with the limit parameter to control paging of a response collection. Example: If limit is 5 and offset is 9, the response returns a collection of 5 objects starting with the 10th object.",
            "required": false,
            "type": "integer",
            "format": "int64"
          },
          {
            "name": "paymentItem",
            "in": "query",
            "description": "The student payment item that is associated with the student payment.",
            "required": false,
            "type": "string"
          },
          {
            "name": "paymentReference",
            "in": "query",
            "description": "The check number or EFT number associated with a student payment.",
            "required": false,
            "type": "string"
          },
          {
            "name": "paymentType",
            "in": "query",
            "description": "The payment type for the reporting transaction.",
            "required": false,
            "type": "string"
          },
          {
            "name": "toDate",
            "in": "query",
            "description": "Limit student payments to payments made on or before this date, using the yyyy-mm-dd format.",
            "required": false,
            "type": "string",
            "format": "date"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response. A successful response can return no matched data.",
            "schema": {
              "type": "object",
              "properties": {
                "data": {
                  "type": "array",
                  "items": {
                    "$ref": "#/definitions/paymentDetail_19886c04bbbc1000186c04303b7e00ff"
