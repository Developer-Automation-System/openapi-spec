        ],
        "responses": {
          "201": {
            "description": "Resource created.",
            "schema": {
              "$ref": "#/definitions/paymentDetail_19886c04bbbc1000186c04303b7e00ff"
            }
          },
          "400": {
            "description": "Invalid request. (https://community.workday.com/rest/error-messages)\n\nChange the Currency to match the default currency for the student's academic unit: [currency]\n\nPayment Type must be one of the Payment Types allowed for the Payment Item.\n\nProvide an Academic Period valid for the student\n\nSubmit a payment amount that is greater than zero.\n\nSubmit an institutional academic unit that the student belongs to.\n\nThe Item used for the Student Payment cannot be marked Inactive.\n\nThe student record no longer exists because it has been merged with another student record. Post the payment for the new student record instead.",
            "schema": {
              "$ref": "#/definitions/VALIDATION_ERROR_MODEL_REFERENCE"
            }
