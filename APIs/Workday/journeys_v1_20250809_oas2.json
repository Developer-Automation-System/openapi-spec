          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "journeyDistributionEventCreateRepresentation",
            "required": true,
            "schema": {
              "$ref": "#/definitions/journeyDistributionEventCreateRepresentation_f18a5110275610002446cd4929990000"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Resource created.",
            "schema": {
              "$ref": "#/definitions/journeyDistributionEventCreateRepresentation_f18a5110275610002446cd4929990000"
            }
          },
          "400": {
            "description": "Invalid request. (https://community.workday.com/rest/error-messages)\nError Code | Validation Message\n--- | ---\nA2642 | You can't distribute the journey builder with category [category] to recipients: [invalidTargets].\nA2643 | Use the Mass Operation Management task in conjunction with the Distribute Journeys Mass Operation Type to distribute journeys to more than 100 targets.\nA2644 | Selected recipients are not active in the system to receive this journey: [invalidTargets].\nA2645 | You must have access to distribute to the following recipients: [insertTargets].\nA2652 | You don't have access to distribute this journey builder.\nA2653 | Specify a journey category that is the same as the journey builder category that you're trying to distribute.\nA2654 | You must use a builder that is ready to distribute.\nA2698 | Specify a valid journey category that matches the selected support targets.\nA2699 | Specify a valid journey target for the related role, such as a manager that supports a worker.\nA2700 | Specify fields for direct distribution or distribution of support journeys, but not for both or none.",
            "schema": {
              "$ref": "#/definitions/VALIDATION_ERROR_MODEL_REFERENCE"
            }
          },
          "401": {
            "description": "Invalid resource or operation. (https://community.workday.com/rest/error-messages)",
            "schema": {
              "$ref": "#/definitions/VALIDATION_ERROR_MODEL_REFERENCE"
